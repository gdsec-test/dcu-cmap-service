{
  "request" : {
    "method" : "GET",
    "urlPath" : "/diablo/v1/accounts",
    "queryParameters" : {
      "addon_domain_eq" : {
        "matches" : "^stub(registered|foreign)(\\d{9}|random)(\\d{9}|random)(diablo)(\\d{9}|random)([0-9A-F]{8}-[0-9A-F]{4}-[4][0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}|random)-.*\\.com$"
      }
    }
  },
  "response" : {
    "status" : 200,
    "bodyFileName": "diablo-hosted-response.json",
    "transformers": ["response-template"]
  }
}